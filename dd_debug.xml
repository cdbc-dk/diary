      <Filename Value="daily_dairy"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir);../../bc_rtl/inc"/>
      <OtherUnitFiles Value="../../bc_rtl;../../bc_rtl/frames;bom;global"/>
      <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)"/>
